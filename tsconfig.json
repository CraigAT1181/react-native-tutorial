{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "expo-env.d.ts",
    "./App.jsx",
    ".expo/types/**/*.ts"
  ],
  "exclude": [
    ".expo"
  ]
}
